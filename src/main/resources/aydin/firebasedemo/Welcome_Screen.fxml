<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aydin.firebasedemo.WelcomeScreenController">
   <children>
      <Label layoutX="217.0" layoutY="53.0" prefHeight="24.0" prefWidth="194.0" text="Welcome to firebasedemo!">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="sign_in" layoutX="240.0" layoutY="213.0" mnemonicParsing="false" onAction="#sign_inClicked" text="Sign In" />
      <Button fx:id="register" layoutX="330.0" layoutY="212.0" mnemonicParsing="false" onAction="#registerClicked" text="Register" />
      <TextField fx:id="usernameTextField" layoutX="104.0" layoutY="104.0" prefHeight="26.0" prefWidth="410.0" promptText="username" />
      <TextField fx:id="passwordTextField" layoutX="105.0" layoutY="145.0" prefHeight="26.0" prefWidth="410.0" promptText="password" />
   </children>
</AnchorPane>
