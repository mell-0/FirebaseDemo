<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@primeTest.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aydin.firebasedemo.WelcomeScreenController">
   <children>
      <Label layoutX="162.0" layoutY="55.0" prefHeight="43.0" prefWidth="242.0" text="Welcome to firebasedemo!">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="sign_in" layoutX="196.0" layoutY="246.0" mnemonicParsing="false" onAction="#sign_inClicked" text="Sign In" />
      <Button fx:id="register" layoutX="300.0" layoutY="246.0" mnemonicParsing="false" onAction="#registerClicked" text="Register" />
      <TextField fx:id="usernameTextField" layoutX="140.0" layoutY="117.0" prefHeight="26.0" prefWidth="410.0" promptText="username" />
      <TextField fx:id="passwordTextField" layoutX="140.0" layoutY="177.0" prefHeight="26.0" prefWidth="410.0" promptText="password" />
   </children>
</AnchorPane>
